#!/usr/bin/env python
import logging
import sys

import zymbit.commands


if __name__ == '__main__':
    try:
        result = zymbit.commands.Command.process_command(zymbit.commands)
    except KeyboardInterrupt:
        pass
    except Exception, exc:
        logger = logging.getLogger('zymbit')
        logger.exception(exc)
    else:
        sys.exit(result)
