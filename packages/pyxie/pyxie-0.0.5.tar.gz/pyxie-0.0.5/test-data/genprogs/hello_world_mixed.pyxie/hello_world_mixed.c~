#include <iostream>
#include <iostream>
#include <iostream>

using namespace std;

int main(int argc, char *argv[])
{
    int second;
    int third;
    int first;
    
std::cout.setf(std::ios::boolalpha);
    first = 1;
    second = 2;
    third = 3;
    cout << first << " " << second << " " << third << endl;
    cout << 1 << " " << 2 << " " << "hello" << endl;
    cout << 1 << " " << 2.1 << " " << 32 << " " << 21 << " " << 100 << " " << true << " " << false << " " << "hello" << 1 << 0 << endl;
    return 0;
}
