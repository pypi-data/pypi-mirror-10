<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="customer.mlscrcom"
    purge="False">

  <configlet
      action_id="customer_mlscrcom"
      appId="customer.mlscrcom"
      category="Products"
      condition_expr=""
      icon_expr="string:$portal_url/logoIcon.png"
      title="mls-cr.com Configuration"
      url_expr="string:${portal_url}/@@mlscrcom-controlpanel"
      visible="True"
      i18n:attributes="title">
